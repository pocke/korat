{
  "name": "korat",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/pocke/korat",
  "author": "Masataka Pocke Kuwabara <kuwabara@pocke.me>",
  "license": "Apache v2",
  "private": true,
  "scripts": {
    "electron": "cross-env KORAT_URL=http://localhost:32034 electron dist/electron.js",
    "start": "npm-run-all clean --parallel server watch",
    "clean": "rimraf public/build dist && mkdirp public/build",
    "server": "cross-env NODE_ENV=development webpack-serve --config config/webpack.renderer.js --port 32034 --content public",
    "watch": "cross-env NODE_ENV=development webpack --watch --config config/webpack.electron.js",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "build:dev": "cross-env NODE_ENV=development npm-run-all build:dev:renderer build:dev:electron",
    "build:dev:renderer": "webpack --config config/webpack.renderer.js",
    "build:dev:electron": "webpack --config config/webpack.electron.js"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@githubprimer/octicons-react": "^8.0.0",
    "@types/lodash-es": "^4.17.1",
    "electron": "^2.0.10",
    "lodash-es": "^4.17.11",
    "react": "^16.5.2",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "jest": "^23.6.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.0.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  }
}

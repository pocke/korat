{
  "name": "korat",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/pocke/korat",
  "author": "Masataka Pocke Kuwabara <kuwabara@pocke.me>",
  "license": "Apache v2",
  "private": true,
  "scripts": {
    "electron": "cross-env KORAT_URL=http://localhost:32034 electron electron.js",
    "start": "npm-run-all clean --parallel server",
    "clean": "rimraf public/build dist && mkdirp public/build",
    "server": "cross-env NODE_ENV=development RUNTIME_TARGET=browser webpack-serve --config webpack.config.js --port 32034 --content public"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "electron": "^2.0.4",
    "query-string": "^6.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/query-string": "^6.1.0",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.2.0",
    "cross-env": "^5.2.0",
    "jest": "^23.4.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2",
    "webpack": "^4.15.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^2.0.2"
  }
}
